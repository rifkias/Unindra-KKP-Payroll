	<queryString>
		<![CDATA[SELECT 
p2.name as periode_name,
p2.start_date,
p2.end_date,
gd.type,
gd.remarks,
(gd.total * -1) as total,
e.employe_name,
e.nik
FROM gaji_detail gd 
LEFT JOIN gaji g on gd.gaji_id = g.gaji_id
LEFT JOIN employe e on g.employe_id = e.employe_id
LEFT JOIN periode p2 on p2.periode_id = g.periode_id
WHERE gd.type = "BPJS"
AND g.periode_id = $P{periode_id}]]>
	</queryString>
